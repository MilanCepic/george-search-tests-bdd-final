{
  "name": "george-search-tests",
  "version": "1.0.0",
  "description": "Automated tests for George banking transaction search (Cypress + Cucumber)",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "test:headless": "cypress run --headless",
    "test:feature": "cypress run --spec 'cypress/features/**/*.feature'"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/features/**/*.js"
    ],
    "nonGlobalStepDefinitions": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MilanCepic/george-search-tests.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MilanCepic/george-search-tests/issues"
  },
  "homepage": "https://github.com/MilanCepic/george-search-tests#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^23.2.1",
    "@cucumber/cucumber": "^12.2.0",
    "cypress": "^14.1.0",
    "esbuild": "^0.27.0"
  }
}
